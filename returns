import java.util.Scanner;
public class returnler {
	static int kare(int a,int b) {
	    int result= 1;
		int i;
		for(i=1;i<=b;i++) {
			result *=a;
			
		}
		return result;
		
	}

	public static void main(String[] args) {
	int d,e,f;
	System.out.println("lütfen hangi sayının üstünü alacaksanız giriniz");
	Scanner scan=new Scanner(System.in);
	e=scan.nextInt();
	System.out.println("lütfen kaçıncı kuvvetini almak istediğini giriniz");
	f=scan.nextInt();
	d= kare(e,f);
	System.out.println(d);

	}

}
