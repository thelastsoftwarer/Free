import java.util.Scanner;
public class Notortalama {

	public static void main(String[] args) {
		int viz,fin,quiz;
		double ortalama;
		Scanner input =new Scanner(System.in);
		System.out.println("please enter quiz notes");
		quiz = input.nextInt();
		System.out.println("please enter vize notes");
		viz = input.nextInt();
		System.out.println("please enter finall notes");
		fin = input.nextInt();
		ortalama=(quiz*0.2)+(viz*0.3)+(fin*0.5);
		System.out.println(ortalama);
		if(ortalama>=60) {
			System.out.println("geçti");
		}
		else {
			System.out.println("kaldı");
		}
		
		
		

	}

}
